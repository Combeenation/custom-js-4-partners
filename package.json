{
	"name": "produkter-customjs",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build-prod": "webpack --progress --colors --config webpack.prod.js",
		"build-dev": "webpack --progress --colors --config webpack.dev.js",
		"build": "npm run build-prod",
		"watch": "npm run watch-dev",
		"watch-dev": "node ./.run-scripts/serve-customjs.js --cfgfile=webpack.dev.js | webpack --progress --colors --config webpack.dev.js --watch",
		"watch-prod": "node ./.run-scripts/serve-customjs.js | webpack --progress --colors --config webpack.prod.js --watch",
		"format-code": "prettier --write \"src\""
	},
	"author": "",
	"devDependencies": {
		"@babel/core": "^7.7.5",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
		"@babel/plugin-proposal-optional-chaining": "^7.7.5",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-preset-env": "^1.7.0",
		"eslint": "^6.8.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-prettier": "^3.1.2",
		"node-static-alias": "^1.1.2",
		"prettier": "^2.0.4",
		"source-map-loader": "^0.2.4",
		"ts-loader": "^6.2.1",
		"typescript": "^3.7.4",
		"webpack": "^4.12.0",
		"webpack-cli": "^3.0.8",
		"webpack-merge": "^4.1.1",
		"webpack-notifier": "^1.6.0"
	},
	"dependencies": {
		"@combeenation/3d-viewer": "1.2.0",
